{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "jest": true,
        "es6": true,
        "node": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": [
            2,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}